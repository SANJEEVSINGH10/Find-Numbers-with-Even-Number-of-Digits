class Solution {
    public int findNumbers(int[] nums) {
        
        
        int evenLength=0;
        int length=1;
        
        for (int i=1 ; i<=nums.length ; i++)
        {
            nums[i] = nums[i]/10;
            length++;
           
             return length;
           }
        if(length%2==0)
        {
            evenLength++;
        }
        return evenLength;
        }
}
